{
  "buildCommand": "",
  "installCommand": "npm install",
  "outputDirectory": "frontend",
  "env": {
    "NODE_ENV": "production"
  },
  "routes": [
    {
      "src": "/src/.*",
      "dest": "/src/$1"
    },
    {
      "src": "/ASSETS/.*",
      "dest": "/ASSETS/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
