{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/src/(.*)",
      "dest": "/frontend/src/$1"
    },
    {
      "src": "^/ASSETS/(.*)",
      "dest": "/frontend/ASSETS/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/index.html"
    }
  ]
}
